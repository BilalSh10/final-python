pipeline {
    agent any
      stages {
        stage('git checkout') {
          steps {
            git branch: 'main', url: 'https://github.com/BilalSh10/final-python'
          }
        }
        stage('build image') {
            steps {
                sh 'docker build -t flask_app_image:latest .'
            }
        }
      }
}
